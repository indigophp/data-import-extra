{
    "name": "indigophp/data-import-extra",
    "description": "Extensions for ddeboer/data-import",
    "license": "MIT",
    "keywords": ["import", "export"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ddeboer/data-import": "dev-master"
    },
    "require-dev": {
        "pagerfanta/pagerfanta": "~1.0",
        "league/csv": "~6.3",
        "phpunit/phpunit": "~4.4"
    },
    "suggest": {
        "pagerfanta/pagerfanta": "Used in Paginated Reader",
        "league/csv": "Used for CSV Reader and Writer"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\DataImport\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
